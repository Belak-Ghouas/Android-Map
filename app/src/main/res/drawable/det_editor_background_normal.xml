<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp">

        <shape android:shape="rectangle">

            <solid android:color="@color/det_underline_color_normal" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/det_line_thickness_normal"
        android:left="0dp"
        android:right="0dp">

        <shape android:shape="rectangle">
            <solid android:color="@color/main_background_color_end" />
        </shape>
    </item>


</layer-list>

